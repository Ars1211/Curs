<Window x:Class="Curs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Curs"
        mc:Ignorable="d"
        Title="{Binding ElementName=AuthFrame, Path=Content.Title}" 
        Height="450" 
        Width="800" 
        Loaded="WindowLoaded"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        Icon="../../icons/MainIcon.png">
    <Grid>
        <Frame x:Name="AuthFrame" NavigationUIVisibility="Hidden" ContentRendered="AuthFrameContentRendered"></Frame>
    </Grid>
</Window>
